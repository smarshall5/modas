@using Modas.Models
@model IEnumerable<Event>
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>Hello</title>
    <script src="https://kit.fontawesome.com/396e86a22b.js" crossorigin="anonymous"></script>
    <script src="https://code.jquery.com/jquery-3.3.1.min.js"
            integrity="sha256-FgpCb/KJQlLNfOu91ta32o/NMZxltwRo8QtmkMRdAu8="
            crossorigin="anonymous"></script>
    <script src="~/js/bootstrap.min.js"></script>
</head>
<body>
    <table class="container table table-bordered">
        <tr><th>Date</th><th>Time</th><th>Location</th></tr>
        @foreach (var e in Model)
        {
            <tr>
                <td>@String.Format("{0: ddd, MMM dd, yyyy}", e.TimeStamp)</td>
                <td>@String.Format("{0:hh:mm tt}", e.TimeStamp)</td>
                <td>@e.Location.Name</td>
            </tr>
        }
    </table>
</body>
</html>